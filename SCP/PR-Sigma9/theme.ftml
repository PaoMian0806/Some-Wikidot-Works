[[code type="css"]]

@import url('https://fonts.googleapis.com/css2?family=Share+Tech+Mono&display=swap');
@import url('https://fonts.googleapis.com/css2?family=ZCOOL+QingKe+HuangYou&display=swap');

:root {
    --box-border-color: orange;
    --box-border-bottom-color: #ccc;
    --boxborder-size: 5px;
}

/* Box Div */

.box-page {
  max-width: 80%;
  margin: auto;
}

.box-container {
  margin: 20px;
  margin-bottom: 0;
  position: relative;
  display: inline-block;
}

.box-body {
  position: relative;
  padding: 10px;
  width: 80%;
  background: #444;
  border-top: var(--boxborder-size) solid var(--box-border-color);
  border-bottom: 2px solid var(--box-border-bottom-color);
  color: white;
  font-family: georgia;
  font-size: 14px;
  line-height: 1.5;
  box-shadow: inset 0 0 var(--box-border-size) #333;
}

.upArrow {
  position: absolute;
  top: var(--box-border-size);
  left: 80%;
  margin-left: -(var(--box-border-size));
  width: 0;
  height: 0;
  border-left: var(--box-border-size) solid transparent;
  border-right: var(--box-border-size) solid transparent;
  border-top: var(--box-border-size) solid var(--box-border-color);
}

/* Date Text Div */

/* .timer-block {
    background: #0f3854;
    background: radial-gradient(ellipse at center,  #0a2e38  0%, #000000 70%);
    background-size: 100%;
} */

.timer-block {
    font-family: Share Tech Mono, monospace;
    color: black;
    text-align: center;
    position: relative;
    text-shadow: 0 0 20px rgba(10, 175, 230, 1.0),  0 0 20px rgba(10, 175, 230, 0);
    height: 100%;
    margin: 15px;
}

    .timer-block .time-text {
        letter-spacing: 0.05em;
        font-size: 36px;
    }
    .timer-block .time-date {
        letter-spacing: 0.05em;
        font-size: 16px;
        padding: 25px;
    }
     .timer-block .normal-text {
        letter-spacing: 0.1em;
        font-size: 12px;
        padding-top: 50px;
    }

/* Note Div 1 */

.notepaper {
  position: relative;
  margin: 30px auto;
  padding: 29px 20px 20px 45px;
  width: 280px;
  line-height: 30px;
  color: #6a5f49;
  text-shadow: 0 1px 1px white;
  background-color: #f2f6c1;
  background-image: -webkit-radial-gradient(center, cover, rgba(255, 255, 255, 0.7) 0%, rgba(255, 255, 255, 0.1) 90%), -webkit-repeating-linear-gradient(top, transparent 0%, transparent 29px, rgba(239, 207, 173, 0.7) 29px, rgba(239, 207, 173, 0.7) 30px);
  background-image: -moz-radial-gradient(center, cover, rgba(255, 255, 255, 0.7) 0%, rgba(255, 255, 255, 0.1) 90%), -moz-repeating-linear-gradient(top, transparent 0%, transparent 29px, rgba(239, 207, 173, 0.7) 29px, rgba(239, 207, 173, 0.7) 30px);
  background-image: -o-radial-gradient(center, cover, rgba(255, 255, 255, 0.7) 0%, rgba(255, 255, 255, 0.1) 90%), -o-repeating-linear-gradient(top, transparent 0%, transparent 29px, rgba(239, 207, 173, 0.7) 29px, rgba(239, 207, 173, 0.7) 30px);
  border: 1px solid #c3baaa;
  border-color: rgba(195, 186, 170, 0.9);
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-box-shadow: inset 0 1px rgba(255, 255, 255, 0.5), inset 0 0 5px #d8e071, 0 0 1px rgba(0, 0, 0, 0.1), 0 2px rgba(0, 0, 0, 0.02);
  box-shadow: inset 0 1px rgba(255, 255, 255, 0.5), inset 0 0 5px #d8e071, 0 0 1px rgba(0, 0, 0, 0.1), 0 2px rgba(0, 0, 0, 0.02);
}

.notepaper:before, .notepaper:after {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
}

.notepaper:before {
 left: 28px;
  width: 2px;
  border: solid #efcfad;
  border-color: rgba(239, 207, 173, 0.9);
  border-width: 0 1px;
}

.notepaper:after {
  z-index: -1;
  left: 0;
  right: 0;
  background: rgba(242, 246, 193, 0.9);
  border: 1px solid rgba(170, 157, 134, 0.7);
  -webkit-transform: rotate(2deg);
  -moz-transform: rotate(2deg);
  -ms-transform: rotate(2deg);
  -o-transform: rotate(2deg);
  transform: rotate(2deg);
}

